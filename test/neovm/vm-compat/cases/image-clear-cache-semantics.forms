(fboundp 'clear-image-cache)
(condition-case err
    (clear-image-cache)
  (error (car err)))
(condition-case err
    (clear-image-cache nil)
  (error (car err)))
(condition-case err
    (clear-image-cache t)
  (error (car err)))
(condition-case err
    (clear-image-cache 1)
  (error (car err)))
(condition-case err
    (clear-image-cache 'foo)
  (error (car err)))
(condition-case err
    (clear-image-cache nil nil)
  (error (car err)))
