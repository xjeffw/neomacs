(with-temp-buffer
  (insert "abcdef")
  (newline)
  (insert "123456")
  (newline)
  (kill-rectangle 1 9)
  killed-rectangle)
(with-temp-buffer
  (setq killed-rectangle '("Q" "W"))
  (insert "abc")
  (newline)
  (insert "def")
  (newline)
  (goto-char 1)
  (yank-rectangle)
  (list (point) (string-to-list (buffer-string))))
(with-temp-buffer
  (setq killed-rectangle nil)
  (insert "abc")
  (goto-char 2)
  (list (yank-rectangle) (point) (string-to-list (buffer-string))))
(condition-case err
    (with-temp-buffer
      (setq killed-rectangle 1)
      (yank-rectangle))
  (error (list (car err) (cadr err))))
(condition-case err
    (with-temp-buffer
      (setq killed-rectangle '("a" 1))
      (yank-rectangle))
  (error (list (car err) (cadr err))))
