; kill-ring-yank-pointer interaction semantics.
(setq kill-ring (list "a" "b" "c"))
(setq kill-ring-yank-pointer kill-ring)
(current-kill 0 t)

(setq kill-ring (list "a" "b" "c"))
(setq kill-ring-yank-pointer (cdr kill-ring))
(current-kill 0 t)

(setq kill-ring (list "a" "b" "c"))
(setq kill-ring-yank-pointer (cdr (cdr kill-ring)))
(current-kill 0 t)

(setq kill-ring (list "a" "b" "c"))
(setq kill-ring-yank-pointer nil)
(condition-case err
    (current-kill 0 t)
  (error (car err)))

(setq kill-ring (list "a" "b" "c"))
(setq kill-ring-yank-pointer (cdr kill-ring))
(current-kill 1 nil)
(car kill-ring-yank-pointer)
