; split-string default separator, omit-nulls, trim, and arg validation
(split-string "a  b")
(split-string "a  b" nil)
(split-string "a  b" " ")
(split-string "a  b" " " t)
(split-string "  a  " nil nil)
(split-string "  a  " nil t)
(split-string "  a  " nil nil "[ ]+")
(split-string "a,b" ",")
(split-string "a,b" "," t)
(condition-case err (split-string "a" 1) (error (car err)))
(condition-case err (split-string "a" nil nil 1) (error (car err)))
(condition-case err (split-string "a" nil nil nil nil) (error (car err)))
