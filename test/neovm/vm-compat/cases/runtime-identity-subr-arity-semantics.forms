(subr-arity (symbol-function 'emacs-version))
(subr-arity (symbol-function 'system-name))

(condition-case err (stringp (emacs-version)) (error err))
(condition-case err (stringp (emacs-version nil)) (error err))
(condition-case err (emacs-version t) (error err))
(condition-case err (emacs-version 1) (error err))
(condition-case err (emacs-version nil nil) (error err))

(condition-case err (stringp (system-name)) (error err))
(condition-case err (system-name nil) (error err))
