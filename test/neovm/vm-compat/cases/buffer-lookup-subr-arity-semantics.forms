(subr-arity (symbol-function 'get-buffer))
(subr-arity (symbol-function 'get-buffer-create))
(subr-arity (symbol-function 'get-file-buffer))
(subr-arity (symbol-function 'generate-new-buffer-name))
(subr-arity (symbol-function 'generate-new-buffer))
(let ((b (get-buffer-create "*vm-blookup*")))
  (bufferp b))
(let ((b (get-buffer-create "*vm-blookup*" nil)))
  (bufferp b))
(let ((b (get-buffer-create "*vm-blookup*" t)))
  (bufferp b))
(condition-case err
    (get-buffer-create "*vm-blookup*" nil nil)
  (error err))
(let ((b (generate-new-buffer "*vm-blookup-g*" nil)))
  (bufferp b))
(let ((b (generate-new-buffer "*vm-blookup-g*" t)))
  (bufferp b))
(condition-case err
    (generate-new-buffer "*vm-blookup-g*" nil nil)
  (error err))
(let ((base "*vm-blookup-name*"))
  (let ((b (get-buffer-create base)))
    (unwind-protect
        (list
         (generate-new-buffer-name base)
         (generate-new-buffer-name base nil)
         (generate-new-buffer-name base t)
         (generate-new-buffer-name base :keyword)
         (generate-new-buffer-name base "*vm-blookup-name*<5>"))
      (kill-buffer b))))
(condition-case err
    (generate-new-buffer-name "*vm-blookup-name*" '(1 2))
  (error err))
(condition-case err
    (generate-new-buffer-name "*vm-blookup-name*" nil nil)
  (error err))
