; transpose-lines should honor dynamic/local/global `buffer-read-only`.
(with-temp-buffer
  (insert (concat "line1" (make-string 1 10) "line2"))
  (goto-char 7)
  (setq buffer-read-only t)
  (condition-case err (transpose-lines 1) (error (car err))))

(with-temp-buffer
  (insert (concat "line1" (make-string 1 10) "line2"))
  (goto-char 7)
  (let ((buffer-read-only t))
    (condition-case err (transpose-lines 1) (error (car err)))))

(let ((orig (default-value 'buffer-read-only)))
  (unwind-protect
      (progn
        (setq-default buffer-read-only t)
        (with-temp-buffer
          (insert (concat "line1" (make-string 1 10) "line2"))
          (goto-char 7)
          (condition-case err
              (progn
                (transpose-lines 1)
                (subst-char-in-string 10 ?| (buffer-string) t))
            (error (car err)))))
    (setq-default buffer-read-only orig)))

(with-temp-buffer
  (insert (concat "line1" (make-string 1 10) "line2"))
  (goto-char 7)
  (transpose-lines 1)
  (subst-char-in-string 10 ?| (buffer-string) t))
