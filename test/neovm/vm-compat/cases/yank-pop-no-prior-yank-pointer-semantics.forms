; yank-pop with explicit yank command context but no prior yank region.
(with-temp-buffer
  (setq kill-ring (list "a" "b" "c"))
  (setq last-command 'yank)
  (condition-case err
      (yank-pop 1)
    (error (list (car err) (car (cdr err)) (car kill-ring-yank-pointer)))))

(with-temp-buffer
  (setq kill-ring (list "a" "b" "c"))
  (setq kill-ring-yank-pointer '(1 2))
  (setq last-command 'yank)
  (condition-case err
      (yank-pop 1)
    (error (list (car err) (car (cdr err)) (car kill-ring-yank-pointer)))))
