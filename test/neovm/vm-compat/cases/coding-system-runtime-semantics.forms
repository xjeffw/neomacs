(check-coding-system 'utf-8)
(condition-case err (check-coding-system 'no-such-coding) (error err))
(condition-case err (check-coding-system "utf-8") (error err))
(define-coding-system-alias 'vm-cs 'utf-8)
(coding-system-p 'vm-cs)
(condition-case err (define-coding-system-alias 'vm-cs2 nil) (error err))
(let ((orig (coding-system-priority-list)))
  (set-coding-system-priority 'raw-text 'utf-8)
  (prog1 (list (car (coding-system-priority-list))
               (car (cdr (coding-system-priority-list))))
    (apply #'set-coding-system-priority orig)))
(let ((orig (coding-system-priority-list)))
  (prog1
      (list
       (condition-case err
           (set-coding-system-priority 'utf-8 'no-such-coding)
         (error err))
       (car (coding-system-priority-list)))
    (apply #'set-coding-system-priority orig)))
(condition-case err (set-coding-system-priority "utf-8") (error err))
