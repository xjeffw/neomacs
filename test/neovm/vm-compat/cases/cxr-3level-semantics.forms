; Core 3-level list accessor semantics.

(caaar '(((1 . 2) . 3) . 4))
(caadr '(x ((1 . 2) . 3) y))
(cadar '(((x . y) (1 . 2) . 3) z))
(caddr '(a b (1 . 2) c))
(cdaar '(((1 . 2) . 3) . 4))
(cdadr '(x ((1 . 2) . 3) y))
(cddar '(((x . y) (1 . 2) . 3) z))
(cdddr '(a b c d))

(list
 (caaar nil) (caadr nil) (cadar nil) (caddr nil)
 (cdaar nil) (cdadr nil) (cddar nil) (cdddr nil))

(condition-case err
    (caaar '(1 . 2))
  (error (list (car err) (car (cdr err)))))

(condition-case err
    (cdddr '(1 . 2))
  (error (list (car err) (car (cdr err)))))
