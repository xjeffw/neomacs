(fboundp 'json-serialize)
(fboundp 'json-parse-string)
(json-serialize [1 2 3])
(json-parse-string "[1,2,3]")
(json-serialize '((a . 1)))
(condition-case err (json-serialize '(("a" . 1))) (error err))
(json-parse-string "{\"a\":1}" :object-type 'alist)
(condition-case err (json-parse-string "") (error (car err)))
(condition-case err (json-parse-string "   ") (error (car err)))
(condition-case err (json-parse-string "42 trailing") (error (car err)))
(condition-case err (json-serialize '(1 . 2)) (error (car err)))
(condition-case err (json-parse-string 1) (error (car err)))
