(fboundp 'minibuffer-contents-no-properties)
(with-temp-buffer
  (insert "probe")
  (minibuffer-contents-no-properties))
(with-temp-buffer
  (insert "probe")
  (equal (minibuffer-contents-no-properties)
         (minibuffer-contents)))
(condition-case err
    (minibuffer-contents-no-properties nil)
  (wrong-number-of-arguments (car err)))
