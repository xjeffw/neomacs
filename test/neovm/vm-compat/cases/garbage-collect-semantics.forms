(fboundp 'garbage-collect)
(let ((gc (garbage-collect)))
  (listp gc))
(let ((gc (garbage-collect)))
  (and (consp (assq 'conses gc))
       (consp (assq 'symbols gc))
       (consp (assq 'strings gc))
       (consp (assq 'buffers gc))))
(let* ((gc (garbage-collect))
       (entry (assq 'conses gc)))
  (and (numberp (nth 1 entry))
       (numberp (nth 2 entry))
       (numberp (nth 3 entry))))
(let* ((gc (garbage-collect))
       (entry (assq 'string-bytes gc)))
  (and (numberp (nth 1 entry))
       (numberp (nth 2 entry))
       (null (nth 3 entry))))
(condition-case err (garbage-collect 1) (wrong-number-of-arguments (car err)))
