; file-name-completion and file-name-all-completions semantics.
(setq vm-fnc-dir
      (let* ((forms-file (getenv "NEOVM_FORMS_FILE"))
             (base (if forms-file (file-name-directory forms-file) default-directory)))
        (expand-file-name "file-name-completion-fixtures" base)))
(file-name-completion "a" vm-fnc-dir)
(file-name-completion "alpha.txt" vm-fnc-dir)
(file-name-completion "al" vm-fnc-dir)
(file-name-completion "z" vm-fnc-dir)
(file-name-completion "." vm-fnc-dir)
(file-name-completion ".." vm-fnc-dir)
(file-name-completion "./" vm-fnc-dir)
(file-name-completion "../" vm-fnc-dir)
(file-name-completion "sub" vm-fnc-dir)
(file-name-completion "subdir/" vm-fnc-dir)
(file-name-all-completions "" vm-fnc-dir)
(file-name-all-completions "." vm-fnc-dir)
(file-name-all-completions ".." vm-fnc-dir)
(file-name-all-completions "sub" vm-fnc-dir)
(file-name-all-completions "subdir/" vm-fnc-dir)
(file-name-all-completions "./" vm-fnc-dir)
