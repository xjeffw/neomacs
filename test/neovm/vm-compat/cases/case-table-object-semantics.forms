(fboundp 'case-table-p)
(fboundp 'current-case-table)
(fboundp 'standard-case-table)
(fboundp 'set-case-table)
(fboundp 'set-standard-case-table)
(case-table-p (current-case-table))
(case-table-p (standard-case-table))
(let ((tbl (current-case-table)))
  (eq (set-case-table tbl) tbl))
(let ((tbl (standard-case-table)))
  (eq (set-standard-case-table tbl) tbl))
(condition-case err
    (set-case-table 1)
  (error (list (car err) (cadr err))))
(condition-case err
    (set-standard-case-table 1)
  (error (list (car err) (cadr err))))
(eq (current-case-table) (standard-case-table))
(let ((orig (current-case-table))
      (tbl (copy-sequence (standard-case-table))))
  (set-case-table tbl)
  (prog1 (list (eq (current-case-table) tbl)
               (eq (current-case-table) (standard-case-table)))
    (set-case-table orig)))
(condition-case err (current-case-table nil) (error (car err)))
(condition-case err (standard-case-table nil) (error (car err)))
