(fboundp 'image-flush)

(condition-case err
    (image-flush nil)
  (error err))
(condition-case err
    (image-flush (create-image "x" 'pbm t))
  (error err))
(condition-case err
    (image-flush (create-image "x" 'pbm t) nil)
  (error err))
(image-flush (create-image "x" 'pbm t) t)
(condition-case err
    (image-flush (create-image "x" 'pbm t) 1)
  (error err))

(condition-case err
    (image-flush (create-image "x" 'pbm t) nil nil)
  (error (car err)))
