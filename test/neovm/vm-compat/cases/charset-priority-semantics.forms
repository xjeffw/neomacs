(condition-case err (set-charset-priority) (error err))
(let ((orig (charset-priority-list)))
  (set-charset-priority 'ascii 'unicode)
  (prog1 (list (car (charset-priority-list))
               (car (cdr (charset-priority-list))))
    (apply #'set-charset-priority orig)))
(condition-case err (set-charset-priority 'no-such-charset) (error err))
(condition-case err (set-charset-priority "ascii") (error err))
(condition-case err (set-charset-priority nil) (error err))
