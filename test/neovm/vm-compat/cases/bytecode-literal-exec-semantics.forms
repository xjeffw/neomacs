; Executable bytecode literals via read-from-string (supported opcode subset).
(funcall (car (read-from-string "#[nil \"\\300\\207\" [0] 1]")))
(funcall (car (read-from-string "#[nil \"\\300\\207\" [42] 1]")))
(funcall (car (read-from-string "#[(x) \"\\10\\207\" [x] 1]")) 99)
(funcall (car (read-from-string "#[(x) \"\\10\\207\" [x] 1]")) 'sym)
(apply (car (read-from-string "#[(x) \"\\10\\207\" [x] 1]")) '(123))

(funcall (car (read-from-string "#[(x) \"\\bT\\207\" [x] 1]")) 77)
(condition-case err
    (funcall (car (read-from-string "#[(x) \"\\bT\\207\" [x] 1]")) 'sym)
  (error err))
